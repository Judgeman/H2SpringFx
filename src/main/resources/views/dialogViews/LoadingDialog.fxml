<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="de.judgeman.H2SpringFx.ViewControllers.DialogControllers.LoadingDialogController">
<children>
      <BorderPane layoutX="10.0" layoutY="10.0" prefHeight="200.0" prefWidth="789.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <center>
            <VBox alignment="CENTER" fillWidth="false" prefHeight="400.0" prefWidth="388.0" BorderPane.alignment="CENTER">
               <children>
                  <BorderPane maxHeight="300.0" maxWidth="400.0" minHeight="50.0" minWidth="300.0" styleClass="dialogBackground">
                     <center>
                        <VBox alignment="BOTTOM_CENTER" spacing="10.0" BorderPane.alignment="CENTER">
                           <BorderPane.margin>
                              <Insets top="20.0" />
                           </BorderPane.margin>
                           <children>
                              <Label fx:id="loadingTextLabel" styleClass="dialogLoadingTextColor" text="Loading Text" />
                           </children>
                        </VBox>
                     </center>
                     <effect>
                        <DropShadow />
                     </effect>
                     <bottom>
                        <HBox alignment="CENTER" BorderPane.alignment="CENTER">
                           <children>
                                      <Button fx:id="cancelButton" cancelButton="true" mnemonicParsing="false" onAction="#cancelButtonClicked" styleClass="dialogLoadingCancelButton" text="%dialog.button.cancel" visible="false" HBox.hgrow="ALWAYS" />
                           </children>
                        </HBox>
                     </bottom>
                     <VBox.margin>
                        <Insets />
                     </VBox.margin>
                     <padding>
                        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
                     </padding>
                  </BorderPane>
               </children>
            </VBox>
         </center>
      </BorderPane>
</children>
</AnchorPane>
